<!DOCTYPE html>
<html>
<head>
  <title>PandSoft </title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  
  <%= csrf_meta_tags %>
</head>
<body>



<div class="navbar-wrapper">
  

        <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
          <div class="container">
            <div class="navbar-header">
              <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <%= link_to "PandSoft Inc" , users_dashboard_path , method: :get, :class => "navbar-brand" %>
            
            </div>
            <div class="navbar-collapse collapse">
              <ul class="nav navbar-nav">
                
                <% if signed_in? %>
                <li><%= link_to "Stories" , users_home_path %></li>
                <li><%= link_to "My Life" , folders_path %></li>
               
                <li><%= link_to "Contribute" , new_charge_path , :method => :get %></li>
                <% end %>
                
                

              </ul>
               <ul class="nav navbar-nav navbar-right">
                <% if signed_in? %>

                <li><%= link_to "signout" ,destroy_user_session_path , :method=>:delete , :class => "btn btn-default"%></li>
                <% else %>
                <li><%= link_to "Sign Up" , new_user_registration_path , :class => "btn btn-default" %></li>
                <li><%= link_to "Sign In" , new_user_session_path , :class => "btn btn-default"%></li>
                <% end %>
                  
                </ul>
            </div>
          </div>
      

      </div>

<% flash.each do |type, message| %>
       <div class=" alert alert-<%= bootstrap_class_for(type) %>">
        <button class="close" data-dismiss="alert">Ã—</button>
        <%= message %>
        </div>
<% end %>
<%= yield %>
